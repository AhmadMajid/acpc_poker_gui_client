:javascript
  rt.finishUpdatingPlayerActionsWindow(
    WindowManager.packageMatchData(
      "#{match_view.match.id}",
      "#{match_view.slice_index}"
    ),
    {
      match_has_ended: 'true' === "#{match_view.match_ended?}",
      next_hand_button_is_visible: 'true' === "#{next_hand_button_visible?}",
      is_users_turn_to_act: 'true' === "#{match_view.slice.users_turn_to_act?}"
    }
  );
